#include<bits/stdc++.h>
using namespace std;
int main(){
 long long int n;
 cin>>n;
 vector<int>v(n);
 for(int i=0;i<n;i++){
    cin>>v[i];

 }



long long int mn=*min_element(v.begin(),v.end());

  long long int freq =count(v.begin(),v.end(),mn);


    if(freq%2!=0){
        cout<<"Lucky"<<"\n";
    }
    else{
        cout<<"Unlucky"<<"\n";
    }



return 0;
}
